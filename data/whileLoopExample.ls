main {
    var int counter1 = 0;
    var int counter2 = 20;
    while( counter1 < counter2 ) {
        counter1 ++;
        counter2 --;
        print("counter1 = ");
        print(counter1);
        print("counter2 = ");
        print(counter2);
        print("\n");
    }
    print("We have reached the End.");
}