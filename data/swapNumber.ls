main {
    var bool flag = false;
    var int num1 = 5;
    var int num2 = 7;
    print("Before Swapping...");
    print("num1 = ");
    print(num1);
    print("num2 = ");
    print(num2);
    var int temp = num1;
    num1 = num2;
    num2 = temp;
    flag = true;
    print("After Swapping...");
    print("num1 = ");
    print(num1);
    print("num2 = ");
    print(num2);
}